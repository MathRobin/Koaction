{
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "globals": {
    "console": true,
    "module": true,
    "require": true,
    "process": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": false
    },
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "comma-spacing": [
      2, {
        "before": false,
        "after": true
      }
    ],
    "complexity": [1, 6],
    "constructor-super": 0,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "generator-star-spacing": 2,
    "id-length": [
      2,
      {
        "min": 3,
        "properties": "always",
        "exceptions": ["id"]
      }
    ],
    "indent": [
      2, 4,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [2, "unix"],
    "max-depth": [2, 6],
    "max-params": [2, 6],
    "max-statements": [2, 25],
    "new-cap": 1,
    "no-alert": 2,
    "no-cond-assign": [2, "always"],
    "no-console": 0,
    "no-const-assign": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-semi": 2,
    "no-extra-strict": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "no-nested-ternary": 2,
    "no-octal": 2,
    "no-plusplus": 2,
    "no-shadow": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 0,
    "no-undef": 2,
    "no-underscore-dangle": 2,
    "no-unreachable": 2,
    "object-shorthand": [0, "never"],
    "prefer-const": 2,
    "quotes": [0, "single"],
    "radix": 2,
    "require-yield": 2,
    "semi": [2, "always"],
    "valid-typeof": 2,
    "vars-on-top": 2,
    "yoda": [2, "always"]
  }
}
